      <tuv lang="ES-VE">
        <seg>El idioma por defecto del portal</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="EN-US">
        <seg>The URL of the current object.</seg>
      </tuv>
      <tuv lang="ES-VE">
        <seg>El URL del objeto actual..</seg>
      </tuv>
    </tu>
